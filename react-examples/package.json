{
  "name": "react-examples",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@types/react-dom": "^18.2.10",
    "@types/react-router": "^5.1.20",
    "concurrently": "^8.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.0.24"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-css": "tailwindcss build -i ./src/input.css -o ./src/assets/output.css",
    "watch-css": "tailwindcss build -i ./src/input.css -o ./src/assets/output.css --watch",
    "dev": "concurrently \"npm run watch-css\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-router-dom": "^6.3.0"
  }
}
